<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_SUB04" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  <bpmn:collaboration id="Collaboration_SUB04">
    <bpmn:participant id="Participant_SUB04" name="SUB 04 - Produção Assistencial e Documentação Clínica" processRef="Process_SUB_04_Clinical_Production" />
  </bpmn:collaboration>
  <bpmn:process id="Process_SUB_04_Clinical_Production" name="Produção Assistencial e Documentação Clínica" isExecutable="true" camunda:historyTimeToLive="P365D">
    <bpmn:laneSet id="LaneSet_SUB04">
      <bpmn:lane id="Lane_Medical_Team" name="Equipe Médica">
        <bpmn:flowNodeRef>Event_Start_Care</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Medical_Eval</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Register_CID</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Create_Orders</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Prescribe</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Medical_Evolution</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Discharge_Summary</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Event_End_Care</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Nursing" name="Enfermagem">
        <bpmn:flowNodeRef>Task_Medication_Admin</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Nursing_Evolution</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Pharmacy" name="Farmácia">
        <bpmn:flowNodeRef>Task_Weight_Sensor</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_IoT_RFID" name="Captura IoT/RFID">
        <bpmn:flowNodeRef>Task_RFID_Capture</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Integration" name="Integração (LIS/PACS)">
        <bpmn:flowNodeRef>Task_LIS_Integration</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_PACS_Integration</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Event_Signal_Discharge</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>

    <bpmn:startEvent id="Event_Start_Care" name="Início Atendimento">
      <bpmn:outgoing>Flow_Start_To_MedEval</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:userTask id="Task_Medical_Eval" name="Avaliação Médica" camunda:assignee="${medico}">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="anamnese" label="Anamnese" type="string" />
          <camunda:formField id="exameFisico" label="Exame Físico" type="string" />
          <camunda:formField id="hipoteseDiagnostica" label="Hipótese Diagnóstica" type="string" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Start_To_MedEval</bpmn:incoming>
      <bpmn:outgoing>Flow_MedEval_To_CID</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:serviceTask id="Task_Register_CID" name="Registrar CID" camunda:asyncBefore="true" camunda:delegateExpression="${registrarCIDDelegate}">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="hipoteseDiagnostica">${hipoteseDiagnostica}</camunda:inputParameter>
          <camunda:inputParameter name="useAISuggestion">true</camunda:inputParameter>
          <camunda:outputParameter name="cidCode">${cidCode}</camunda:outputParameter>
          <camunda:outputParameter name="aiConfidence">${aiConfidence}</camunda:outputParameter>
        </camunda:inputOutput>
      
        <camunda:executionListener event="start" class="com.hospital.audit.TaskStartListener" />
        <camunda:executionListener event="end" class="com.hospital.audit.TaskEndListener" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_MedEval_To_CID</bpmn:incoming>
      <bpmn:outgoing>Flow_CID_To_Orders</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:userTask id="Task_Create_Orders" name="Criar Ordens de Serviço" camunda:assignee="${medico}">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="examesLaboratoriais" label="Exames Laboratoriais" type="string" />
          <camunda:formField id="examesImagem" label="Exames de Imagem" type="string" />
          <camunda:formField id="procedimentos" label="Procedimentos" type="string" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_CID_To_Orders</bpmn:incoming>
      <bpmn:outgoing>Flow_Orders_To_Prescribe</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:userTask id="Task_Prescribe" name="Prescrição" camunda:assignee="${medico}">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="medicamentos" label="Medicamentos" type="string" />
          <camunda:formField id="posologia" label="Posologia" type="string" />
          <camunda:formField id="viaAdministracao" label="Via de Administração" type="string" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Orders_To_Prescribe</bpmn:incoming>
      <bpmn:outgoing>Flow_Prescribe_To_RFID</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:serviceTask id="Task_RFID_Capture" name="Captura RFID Materiais" camunda:asyncBefore="true" camunda:type="external" camunda:topic="iot-rfid-capture">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="patientId">${patientId}</camunda:inputParameter>
          <camunda:outputParameter name="materialsUsed">${materialsUsed}</camunda:outputParameter>
        </camunda:inputOutput>
      
        <camunda:executionListener event="start" class="com.hospital.audit.TaskStartListener" />
        <camunda:executionListener event="end" class="com.hospital.audit.TaskEndListener" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Prescribe_To_RFID</bpmn:incoming>
      <bpmn:outgoing>Flow_RFID_To_Weight</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_Weight_Sensor" name="Sensor de Peso Dispensário" camunda:asyncBefore="true" camunda:type="external" camunda:topic="iot-weight-sensor">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="prescriptionId">${prescriptionId}</camunda:inputParameter>
          <camunda:outputParameter name="medicationDispensed">${medicationDispensed}</camunda:outputParameter>
        </camunda:inputOutput>
      
        <camunda:executionListener event="start" class="com.hospital.audit.TaskStartListener" />
        <camunda:executionListener event="end" class="com.hospital.audit.TaskEndListener" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_RFID_To_Weight</bpmn:incoming>
      <bpmn:outgoing>Flow_Weight_To_MedAdmin</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:userTask id="Task_Medication_Admin" name="Administrar Medicação" camunda:assignee="${enfermeiro}">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="codigoBarras" label="Código de Barras" type="string" />
          <camunda:formField id="horarioAdministracao" label="Horário" type="string" />
          <camunda:formField id="observacoes" label="Observações" type="string" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Weight_To_MedAdmin</bpmn:incoming>
      <bpmn:outgoing>Flow_MedAdmin_To_NursEvo</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:userTask id="Task_Nursing_Evolution" name="Evolução Enfermagem" camunda:assignee="${enfermeiro}">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="evolucaoEnfermagem" label="Evolução" type="string" />
          <camunda:formField id="sinaisVitais" label="Sinais Vitais" type="string" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_MedAdmin_To_NursEvo</bpmn:incoming>
      <bpmn:outgoing>Flow_NursEvo_To_LIS</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:serviceTask id="Task_LIS_Integration" name="Integrar Resultados Lab" camunda:asyncBefore="true" camunda:delegateExpression="${lisIntegrationDelegate}">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="patientId">${patientId}</camunda:inputParameter>
          <camunda:outputParameter name="labResults">${labResults}</camunda:outputParameter>
        </camunda:inputOutput>
      
        <camunda:executionListener event="start" class="com.hospital.audit.TaskStartListener" />
        <camunda:executionListener event="end" class="com.hospital.audit.TaskEndListener" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_NursEvo_To_LIS</bpmn:incoming>
      <bpmn:outgoing>Flow_LIS_To_PACS</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_PACS_Integration" name="Integrar Laudos Imagem" camunda:asyncBefore="true" camunda:delegateExpression="${pacsIntegrationDelegate}">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="patientId">${patientId}</camunda:inputParameter>
          <camunda:outputParameter name="imagingReports">${imagingReports}</camunda:outputParameter>
        </camunda:inputOutput>
      
        <camunda:executionListener event="start" class="com.hospital.audit.TaskStartListener" />
        <camunda:executionListener event="end" class="com.hospital.audit.TaskEndListener" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_LIS_To_PACS</bpmn:incoming>
      <bpmn:outgoing>Flow_PACS_To_MedEvo</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:userTask id="Task_Medical_Evolution" name="Evolução Médica" camunda:assignee="${medico}">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="evolucaoMedica" label="Evolução Médica" type="string" />
          <camunda:formField id="condutaMedica" label="Conduta" type="string" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_PACS_To_MedEvo</bpmn:incoming>
      <bpmn:outgoing>Flow_MedEvo_To_Signal</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:intermediateCatchEvent id="Event_Signal_Discharge" name="Sinal de Alta">
      <bpmn:incoming>Flow_MedEvo_To_Signal</bpmn:incoming>
      <bpmn:outgoing>Flow_Signal_To_Summary</bpmn:outgoing>
      <bpmn:signalEventDefinition signalRef="Signal_Discharge" />
    </bpmn:intermediateCatchEvent>

    <bpmn:userTask id="Task_Discharge_Summary" name="Sumário de Alta" camunda:assignee="${medico}">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="diagnosticoFinal" label="Diagnóstico Final" type="string" />
          <camunda:formField id="prescricaoAlta" label="Prescrição de Alta" type="string" />
          <camunda:formField id="orientacoes" label="Orientações" type="string" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Signal_To_Summary</bpmn:incoming>
      <bpmn:outgoing>Flow_Summary_To_End</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:endEvent id="Event_End_Care" name="Atendimento Concluído">
      <bpmn:incoming>Flow_Summary_To_End</bpmn:incoming>
    </bpmn:endEvent>

    <!-- Event Subprocess for Concurrent Audit -->
    <bpmn:subProcess id="SubProcess_Concurrent_Audit" triggeredByEvent="true">
      <bpmn:startEvent id="Event_Audit_Timer" name="Timer Auditoria">
        <bpmn:outgoing>Flow_Timer_To_Rules</bpmn:outgoing>
        <bpmn:timerEventDefinition>
          <bpmn:timeCycle>R/PT1H</bpmn:timeCycle>
        </bpmn:timerEventDefinition>
      </bpmn:startEvent>
      <bpmn:serviceTask id="Task_Run_Audit_Rules" name="Executar Regras de Auditoria" camunda:delegateExpression="${auditRulesDelegate}">
        <bpmn:extensionElements>
          <camunda:executionListener event="start" class="com.hospital.audit.TaskStartListener" />
          <camunda:executionListener event="end" class="com.hospital.audit.TaskEndListener" />
        </bpmn:extensionElements>
        <bpmn:incoming>Flow_Timer_To_Rules</bpmn:incoming>
        <bpmn:outgoing>Flow_Rules_To_Gateway</bpmn:outgoing>
      </bpmn:serviceTask>
      <bpmn:exclusiveGateway id="Gateway_Audit_Issues" name="Inconsistências?">
        <bpmn:incoming>Flow_Rules_To_Gateway</bpmn:incoming>
        <bpmn:outgoing>Flow_Issues_Yes</bpmn:outgoing>
        <bpmn:outgoing>Flow_Issues_No</bpmn:outgoing>
      </bpmn:exclusiveGateway>
      <bpmn:serviceTask id="Task_Create_Alert" name="Criar Alerta" camunda:delegateExpression="${createAlertDelegate}">
        <bpmn:extensionElements>
          <camunda:executionListener event="start" class="com.hospital.audit.TaskStartListener" />
          <camunda:executionListener event="end" class="com.hospital.audit.TaskEndListener" />
        </bpmn:extensionElements>
        <bpmn:incoming>Flow_Issues_Yes</bpmn:incoming>
        <bpmn:outgoing>Flow_Alert_To_End</bpmn:outgoing>
      </bpmn:serviceTask>
      <bpmn:endEvent id="Event_Audit_End" name="Auditoria Concluída">
        <bpmn:incoming>Flow_Alert_To_End</bpmn:incoming>
        <bpmn:incoming>Flow_Issues_No</bpmn:incoming>
      </bpmn:endEvent>
      <bpmn:sequenceFlow id="Flow_Timer_To_Rules" sourceRef="Event_Audit_Timer" targetRef="Task_Run_Audit_Rules" />
      <bpmn:sequenceFlow id="Flow_Rules_To_Gateway" sourceRef="Task_Run_Audit_Rules" targetRef="Gateway_Audit_Issues" />
      <bpmn:sequenceFlow id="Flow_Issues_Yes" name="Sim" sourceRef="Gateway_Audit_Issues" targetRef="Task_Create_Alert">
        <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${hasIssues == true}</bpmn:conditionExpression>
      </bpmn:sequenceFlow>
      <bpmn:sequenceFlow id="Flow_Issues_No" name="Não" sourceRef="Gateway_Audit_Issues" targetRef="Event_Audit_End">
        <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${hasIssues == false}</bpmn:conditionExpression>
      </bpmn:sequenceFlow>
      <bpmn:sequenceFlow id="Flow_Alert_To_End" sourceRef="Task_Create_Alert" targetRef="Event_Audit_End" />
    </bpmn:subProcess>

    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_Start_To_MedEval" sourceRef="Event_Start_Care" targetRef="Task_Medical_Eval" />
    <bpmn:sequenceFlow id="Flow_MedEval_To_CID" sourceRef="Task_Medical_Eval" targetRef="Task_Register_CID" />
    <bpmn:sequenceFlow id="Flow_CID_To_Orders" sourceRef="Task_Register_CID" targetRef="Task_Create_Orders" />
    <bpmn:sequenceFlow id="Flow_Orders_To_Prescribe" sourceRef="Task_Create_Orders" targetRef="Task_Prescribe" />
    <bpmn:sequenceFlow id="Flow_Prescribe_To_RFID" sourceRef="Task_Prescribe" targetRef="Task_RFID_Capture" />
    <bpmn:sequenceFlow id="Flow_RFID_To_Weight" sourceRef="Task_RFID_Capture" targetRef="Task_Weight_Sensor" />
    <bpmn:sequenceFlow id="Flow_Weight_To_MedAdmin" sourceRef="Task_Weight_Sensor" targetRef="Task_Medication_Admin" />
    <bpmn:sequenceFlow id="Flow_MedAdmin_To_NursEvo" sourceRef="Task_Medication_Admin" targetRef="Task_Nursing_Evolution" />
    <bpmn:sequenceFlow id="Flow_NursEvo_To_LIS" sourceRef="Task_Nursing_Evolution" targetRef="Task_LIS_Integration" />
    <bpmn:sequenceFlow id="Flow_LIS_To_PACS" sourceRef="Task_LIS_Integration" targetRef="Task_PACS_Integration" />
    <bpmn:sequenceFlow id="Flow_PACS_To_MedEvo" sourceRef="Task_PACS_Integration" targetRef="Task_Medical_Evolution" />
    <bpmn:sequenceFlow id="Flow_MedEvo_To_Signal" sourceRef="Task_Medical_Evolution" targetRef="Event_Signal_Discharge" />
    <bpmn:sequenceFlow id="Flow_Signal_To_Summary" sourceRef="Event_Signal_Discharge" targetRef="Task_Discharge_Summary" />
    <bpmn:sequenceFlow id="Flow_Summary_To_End" sourceRef="Task_Discharge_Summary" targetRef="Event_End_Care" />
  </bpmn:process>

  <bpmn:signal id="Signal_Discharge" name="Signal_Discharge" />

  <bpmndi:BPMNDiagram id="BPMNDiagram_SUB04">
    <bpmndi:BPMNPlane id="BPMNPlane_SUB04" bpmnElement="Collaboration_SUB04">
      <bpmndi:BPMNShape id="Participant_SUB04_di" bpmnElement="Participant_SUB04" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="2800" height="900" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_Medical_Team_di" bpmnElement="Lane_Medical_Team" isHorizontal="true">
        <dc:Bounds x="190" y="80" width="2770" height="250" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_Nursing_di" bpmnElement="Lane_Nursing" isHorizontal="true">
        <dc:Bounds x="190" y="330" width="2770" height="150" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_Pharmacy_di" bpmnElement="Lane_Pharmacy" isHorizontal="true">
        <dc:Bounds x="190" y="480" width="2770" height="150" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_IoT_RFID_di" bpmnElement="Lane_IoT_RFID" isHorizontal="true">
        <dc:Bounds x="190" y="630" width="2770" height="150" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_Integration_di" bpmnElement="Lane_Integration" isHorizontal="true">
        <dc:Bounds x="190" y="780" width="2770" height="200" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Event_Start_Care_di" bpmnElement="Event_Start_Care">
        <dc:Bounds x="252" y="152" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="239" y="195" width="62" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Medical_Eval_di" bpmnElement="Task_Medical_Eval">
        <dc:Bounds x="340" y="130" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Register_CID_di" bpmnElement="Task_Register_CID">
        <dc:Bounds x="490" y="130" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Create_Orders_di" bpmnElement="Task_Create_Orders">
        <dc:Bounds x="640" y="130" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Prescribe_di" bpmnElement="Task_Prescribe">
        <dc:Bounds x="790" y="130" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Medical_Evolution_di" bpmnElement="Task_Medical_Evolution">
        <dc:Bounds x="2080" y="130" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Discharge_Summary_di" bpmnElement="Task_Discharge_Summary">
        <dc:Bounds x="2480" y="130" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Event_End_Care_di" bpmnElement="Event_End_Care">
        <dc:Bounds x="2642" y="152" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="2627" y="195" width="66" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Medication_Admin_di" bpmnElement="Task_Medication_Admin">
        <dc:Bounds x="1190" y="360" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Nursing_Evolution_di" bpmnElement="Task_Nursing_Evolution">
        <dc:Bounds x="1340" y="360" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Weight_Sensor_di" bpmnElement="Task_Weight_Sensor">
        <dc:Bounds x="1040" y="510" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_RFID_Capture_di" bpmnElement="Task_RFID_Capture">
        <dc:Bounds x="890" y="660" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_LIS_Integration_di" bpmnElement="Task_LIS_Integration">
        <dc:Bounds x="1640" y="810" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_PACS_Integration_di" bpmnElement="Task_PACS_Integration">
        <dc:Bounds x="1790" y="810" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Event_Signal_Discharge_di" bpmnElement="Event_Signal_Discharge">
        <dc:Bounds x="2312" y="832" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="2299" y="875" width="62" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="Flow_Start_To_MedEval_di" bpmnElement="Flow_Start_To_MedEval">
        <di:waypoint x="288" y="170" />
        <di:waypoint x="340" y="170" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_MedEval_To_CID_di" bpmnElement="Flow_MedEval_To_CID">
        <di:waypoint x="440" y="170" />
        <di:waypoint x="490" y="170" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_CID_To_Orders_di" bpmnElement="Flow_CID_To_Orders">
        <di:waypoint x="590" y="170" />
        <di:waypoint x="640" y="170" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_Orders_To_Prescribe_di" bpmnElement="Flow_Orders_To_Prescribe">
        <di:waypoint x="740" y="170" />
        <di:waypoint x="790" y="170" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_Prescribe_To_RFID_di" bpmnElement="Flow_Prescribe_To_RFID">
        <di:waypoint x="840" y="210" />
        <di:waypoint x="840" y="700" />
        <di:waypoint x="890" y="700" />
      </bpmndi:BPMNEdge>
